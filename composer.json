{
    "name": "mapo-89/heizreportapi",
    "description": "This package make a connection to the heizreport api and let you interact with it.",
    "keywords": [
        "mapo-89",
        "heizreportapi"
    ],
    "homepage": "https://github.com/mapo-89/heizreportapi",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Manuel Postler",
            "email": "info@postler.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mapo89\\HeizreportApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mapo89\\HeizreportApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mapo89\\HeizreportApi\\HeizreportApiServiceProvider"
            ],
            "aliases": {
                "HeizreportApi": "Mapo89\\HeizreportApi\\HeizreportApiFacade"
            }
        }
    }
}
